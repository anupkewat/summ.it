{
  "manifest_version": 2,
  "name": "Summ.it",
  "version": "1.0",
  "description": "Jzt Summit baby!",
   "permissions": [
      "activeTab",
      "storage"
  ],
  "icons": {
      "16" : "icons/16x16.png",
      "48" : "icons/48x48.png",
      "128": "icons/128x128.png"
  },
  "browser_action": {
      "default_icon": "icons/48x48.png",
      "default_popup": "popup.html",
      "default_title": "Summ.it"
  },
  "background": {
      "scripts": [
          "background.js"
          
      ],
      "persistent": false
      }
  }